{
  "type": "module",
  "name": "react-dnd-provider",
  "version": "0.1.20",
  "description": "A lightweight drag and drop provider library for react having TypeScript support",
  "author": "Mukesh Maurya",
  "homepage": "https://maurya20.github.io/react-dnd-provider/",
  "keywords": [
    "react-dnd-provider",
    "drag and drop",
    "dnd library",
    "kanbon drag and drop",
    "sortable",
    "reorder",
    "reorderable",
    "react drag and drop",
    "react dnd provider",
    "beautiful-dnd alternative"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maurya20/react-dnd-provider.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT"
}