{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "lib": ["ES2019", "DOM"]
  },
  "include": ["src"]
}
